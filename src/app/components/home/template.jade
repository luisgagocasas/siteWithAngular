.container
    .row
        div(uib-carousel, active="active", interval="myInterval", no-wrap="noWrapSlides")
            div(uib-slide, ng-repeat="slide in slides track by slide.id", index="slide.id")
                img(ng-src="{{slide.image}}", style="margin:auto;")
                div(class="carousel-caption")
                    h4 Slide {{slide.id}}
                    p {{slide.text}}

.container
    .row
        .jumbotron
            h1.text-center MundiCar
            p Lorem ipsum dolor sit amet, consectetur adipisicing elit. Repellendus dolorem ad officiis nihil delectus quidem animi rerum, molestias sint natus ducimus quod, unde molestiae dolores laboriosam error facilis odio doloremque.</p>
            p
                //a(href="#", class="btn btn-primary btn-lg", role="button") Leer mas

.container
    .row
        .col-xs-12.col-sm-6.col-md-3(ng-repeat="categorias_principal in categorias_principales")
            .thumbnail
                img(src="data:image/svg+xml;base64,")
            .caption
                ul.list-group
                    li.list-group-item.active {{ categorias_principal.nombre }}
                    li.list-group-item(ng-repeat="categorias_secundaria in categorias_secundarias")
                        span.badge 2
                        | {{categorias_secundaria.nombre}}
            
.container
    .row
        .col-xs-12.col-sm-6.col-md-4(ng-repeat="publicacion in publicaciones")
            //h2 {{ publicacion.cod }}
            figure
                img(src="http://iglobehost.com/mundicar/oc-content/uploads/0/17_thumbnail.jpg")
            p {{ publicacion.mrc }} - {{ publicacion.modelo }}
            a(href="#", class="btn btn-success") Leer mas

